   "source": [
    "class Number: \n",
    "    def __init__(self,x,n):\n",
    "        self.x = x\n",
    "        self.n = n \n",
    "\n",
    "    def answer(self):\n",
    "        print('the base number is:',self.x)\n",
    "        print('the power number is:',self.n)\n",
    "        print('the answer is:',pow(self.x,self.n))\n",
    "\n",
    "n1 = Number(10,2)\n",
    "n1.answer()\n",
    "\n"
   ]
